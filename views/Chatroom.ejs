<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Seeme</title>
    <link rel="stylesheet" href="layout.css">
    <script src="/socket.io/socket.io.js" ></script>
    <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
    <script src="https://kit.fontawesome.com/ae6847aa03.js" crossorigin="anonymous"></script>
    <script>

        const ROOM_ID = "<%= roomid %>";  
    </script>
</head>
<body>
    

    <div class="main">
        <div class="left_screen">
            <div class="video_stream">
                <div id="show"></div>
            </div>
            <div class="control"> 
                <div class="control_part">
                    <div class="button1">
                        <i class="fas fa-microphone-alt-slash"></i>
                        <span>Mute</span>
                    </div>
                    <div class="button1">
                        <i class="fas fa-video-slash"></i>
                        <span>Quit video</span>
                    </div>
                </div>
                <div class="control_part">
                    <div class="button2">
                        <i class="fas fa-shield-alt"></i>
                        <span>Security</span>
                    </div>
                    <div class="button2">
                        <i class="fas fa-users"></i>
                        <span>Participant</span>
                    </div>
                    <div class="button2">
                        <i class="far fa-comments"></i>
                        <span>Chat</span>
                    </div>
                </div>
                <div class="control_part">
                    <div class="button">
                      <span>Leave Chat</span>
                    </div>
                </div>
            </div>

        </div>
        <div class="right_screen">
            <div class="chat">
                <H6>Chat</H6>
            </div>

        </div>
    </div>
</body>
<script src="com.js"></script>
</html>